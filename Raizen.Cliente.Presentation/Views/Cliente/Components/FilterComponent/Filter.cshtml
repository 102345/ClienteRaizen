@model Raizen.Cliente.Application.Contracts.FiltroClienteModel
@*
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
<link href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css' rel='stylesheet' type='text/css'>
<script src='https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.min.js' type='text/javascript'></script>*@

<div class="col-xs-12">
    <form method="POST" enctype="multipart/form-data" asp-controller="Cliente" asp-action="FilterCliente">
        <div class="row">
            <div class="col">
                <label>Nome</label>
            </div>
            <div class="col">
                <label>Email</label>
            </div>
            <div class="col">
                <label>Cidade</label>
            </div>
            <div class="col"></div>
        </div>
        <div class="row">
            <div class="col">
                <input type="text" class="form-control" id="Nome" name="Nome" asp-for="Nome" />
            </div>
         
            <div class="col">
                <input type="text" class="form-control" id="Email" name="Email" asp-for="Email">
                
            </div>

            <div class="col">
                <input type="text" class="form-control" id="Cidade" name="Cidade" asp-for="Cidade">
            </div>

            <div class="col">
                <button type="submit" class="btn btn-primary" value="Search" id="btnSearch">
                    <svg bootstrap-icon="Search" width="24" height="24"></svg>
                    Pesquisar
                </button>
            </div>
        </div>
    </form>
</div>